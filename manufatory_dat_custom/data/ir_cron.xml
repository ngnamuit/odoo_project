<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <!-- Scheduler for product standard price computing-->
        <record forcecreate="True" id="ir_cron_mrp_bom_product_standard_price_computing" model="ir.cron">
            <field name="name">Automation: Compute Price from BoM</field>
            <field name="model_id" ref="model_mrp_bom"/>
            <field name="state">code</field>
            <field name="code">model.compute_product_bom_stand_price()</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">20</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
        </record>
    </data>
</odoo>